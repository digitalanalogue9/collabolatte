{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "collabolatte": {
      "appLocation": "apps\\web\\dist",
      "apiLocation": "apps\\api\\src",
      "outputLocation": ".",
      "apiLanguage": "dotnetisolated",
      "apiVersion": "10.0",
      "packageManager": "pnpm",
      "appBuildCommand": "pnpm --filter @collabolatte/web build",
      "apiBuildCommand": "dotnet publish -c Release",
      "run": "pnpm --filter @collabolatte/web dev",
      "appDevserverUrl": "http://localhost:3000"
    }
  }
}
