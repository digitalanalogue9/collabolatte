{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "collabolatte": {
      "appLocation": "apps\\web",
      "apiLocation": "apps\\api\\src",
      "outputLocation": "dist",
      "apiLanguage": "dotnetisolated",
